<Page x:Class="MenuWithSubMenu.Pages.Dashboard"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MenuWithSubMenu.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Dashboard">

   
    <Grid>
        <StackPanel VerticalAlignment="Center">
            <TextBlock FontSize="30" Text="Dashboard" HorizontalAlignment="Center" VerticalAlignment="Center"/>
            <Button x:Name="BtnNew"
                    Width="50"
                    Height="30"
                    Content="New"
                    Command="{Binding MenuCommand}"
                    CommandParameter="New User"
                />

        </StackPanel>
    </Grid>
</Page>
